{
  "name": "@fit-screen/vue",
  "type": "module",
  "version": "0.0.0",
  "private": false,
  "author": "jp-liu <fortune_coder@foxmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/jp-liu/@fit-screen/vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.jp-liu/@fit-screen",
    "directory": "./packages/vue"
  },
  "bugs": "https://github.com/jp-liu/@fit-screen#issues",
  "keywords": [
    "vuejs",
    "vue",
    "vue-components",
    "vue2",
    "vue3",
    "vue-plugin",
    "vue-component",
    "vue-resource"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js"
    }
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:analyze": "vite build --mode=analyze",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.1",
    "vue": "^2.6.14 || ^3.2.39"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@fit-screen/shared": "workspace:^0.0.0",
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^1.7.2",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.24"
  }
}
